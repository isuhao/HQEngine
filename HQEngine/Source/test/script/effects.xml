<techniques>
	<technique name="mesh-effect"> <!-- must specify technique name -->
		<pass name="pass-0">
			<vertex_shader>vs-mesh</vertex_shader>
			<pixel_shader>ps-mesh</pixel_shader>
			<texture0
				min_filter="linear"
				mag_filter="linear"
				mipmap_filter="none"
			>
			Bat_Albedo.jpg
			</texture0>
			<stencil>
				<reference_value>1</reference_value> <!-- decimal value -->
				<compare_func>less</compare_func> <!-- default is always -->
			</stencil>
			<depth>full</depth> <!-- default is none -->
			<blend>
				<src_factor>one minus src alpha</src_factor>
				<dest_factor>src alpha</dest_factor>
			</blend>
		</pass>
	</technique>
</techniques>